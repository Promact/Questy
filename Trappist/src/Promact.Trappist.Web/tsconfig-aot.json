{
  "compilerOptions": {
    "baseUrl": "",
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./build",
    "skipLibCheck": true,
    "typeRoots": [ "./node_modules/@types" ],
    "lib": [ "es6", "dom" ]
  },
  "compileOnSave": false,
  "buildOnSave": false,
  "files": [
    "wwwroot/app/app.module.ts",
    "wwwroot/app/conduct/conduct.module.ts",
    "wwwroot/app/setup/setup.module.ts",
    "wwwroot/app/main.aot.ts",
    "wwwroot/app/main.conduct.aot.ts",
    "wwwroot/app/main.setup.aot.ts"
  ],
  "awesomeTypescriptLoaderOptions": {
    "forkChecker": true,
    "useWebpackText": true
  },
  "angularCompilerOptions": {
    "genDir": ".",
    "entryModule": "wwwroot/app/app.module#AppModule",
    "skipMetadataEmit": true
  }
}