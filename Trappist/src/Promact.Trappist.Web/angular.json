{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "wwwroot",
      "sourceRoot": "wwwroot/app",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "wwwroot/dist",
            "index": "wwwroot/app/app.html",
            "main": "wwwroot/app/main.ts",
            "polyfills": "wwwroot/polyfills.ts",
            "tsConfig": "wwwroot/tsconfig.json",
            "assets": [],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "./wwwroot/css/style.css"
            ],
            "scripts": [],
            "deployUrl": "wwwroot/dist"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "wwwroot/app/environments/environment.ts",
                  "with": "wwwroot/app/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "none",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true
            },
            "hmr": {
              "fileReplacements": [
                {
                  "replace": "wwwroot/app/environments/environment.ts",
