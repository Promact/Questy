              "namedChunks": false,
              "aot": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "wwwroot/tsconfig.json"              
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
